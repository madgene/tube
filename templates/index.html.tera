{% extends "base" %}

{% block content %}
<h2>Link:</h2>
<form action="/" method="post">
<input type="text" name="url" placeholder="Input the url">
<input type="submit" value="Download">
<ul>
{% for file in files -%}
<li><a href="{{ file.url }}">{{ file.name }}</a></li>
{%- endfor %}
</ul>
</form>
{% endblock content %}
